{
	"name": "restaurant-tutorial",
	"version": "1.0.0",
	"description": "Tutorial for restaurant site using MERN stack",
	"main": "server.js",
	"engines": {
		"node": "18.18.0",
		"npm": "9.8.1"
	},
	"scripts": {
		"start": "node server.js",
		"start-server": "nodemon server.js --ignore './client/'",
		"start-client": "npm start --prefix client",
		"dev": "concurrently \"npm run start-server\" \"npm run start-client\""
	},
	"author": "Jorge Alvarado",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.6.2",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"json": "^11.0.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^7.1.0",
		"multer": "^1.4.4",
		"nodemon": "^2.0.22",
		"react-redux": "^8.1.3",
		"redux": "^4.2.1",
		"redux-devtools-extension": "^2.13.9",
		"redux-thunk": "^2.4.2",
		"stripe": "^12.18.0",
		"token": "^0.1.0",
		"web": "^0.0.2"
	},
	"devDependencies": {
		"concurrently": "^8.0.1",
		"minimist": "^1.2.8",
		"morgan": "^1.10.0"
	}
}
